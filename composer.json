{
    "name": "mrsbuttercup/unplug",
    "description": "Unplug stuff",
    "keywords": [
        "Telegram",
        "Telegram Bot",
        "Unplug"
    ],
    "type": "project",
    "authors": [
        {
            "name": "Mary De NÃ³brega",
            "email": "denobrega.mary@gmail.com",
            "homepage": "https://marydenobrega.com",
            "role": "Developer"
        }
    ],
    "license": "proprietary",
    "require": {
        "php": "^7.4.2",

        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-http": "*",
        "ext-iconv": "*",
        "ext-json": "*",

        "doctrine/inflector": "^1.3",
        "sentry/sentry-symfony": "^3.4",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.0.*",
        "symfony/framework-bundle": "5.0.*",
        "symfony/http-client": "5.0.*",
        "symfony/twig-pack": "^1.0",
        "symfony/yaml": "5.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/browser-kit": "5.0.*",
        "symfony/css-selector": "5.0.*",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "apcu-autoloader": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4.2"
        },
        "process-timeout": 600
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.0.*"
        }
    }
}
